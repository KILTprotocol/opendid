{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "plugin:import/recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/no-unused-vars": "error"
    },
    "settings": {
        "import/resolver": {
            "typescript": {}
        }
    }
}
